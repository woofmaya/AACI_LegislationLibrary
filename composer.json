{
  "require": {
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "^2.3",
    "doublesecretagency/craft-cpcss": "2.1.0",
    "ether/tags": "1.0.1",
    "mmikkel/child-me": "^1.0",
    "mmikkel/cp-field-inspect": "1.0.5",
    "spicyweb/craft-embedded-assets": "2.0.2",
    "verbb/cp-nav": "^2.0",
    "verbb/expanded-singles": "1.0.7",
    "verbb/field-manager": "2.0.5",
    "verbb/super-table": "2.1.16",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
